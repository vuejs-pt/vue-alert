{
  "name": "@vuejs-pt/vue-alert",
  "version": "1.2.2",
  "description": "Alert bootstrap style for Vue.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs-pt/vue-alert.git"
  },
  "author": "David PiÃ§arra (https://www.davidpicarra.com/)",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "rollup -c config/rollup.config.js",
    "build:example": "NODE_ENV=production node config/build.js",
    "clean": "rimraf dist/*",
    "codecov": "codecov",
    "dev": "cross-env NODE_ENV=development node config/dev-server.js",
    "lint": "eslint --ext .js,.vue .",
    "test": "npm run lint && jest --coverage",
    "test:dev": "jest --watchAll",
    "prebuild": "npm run clean -s",
    "preversion": "npm run build && npm run test && npm run build:example",
    "version": "git add -A dist && git add -A example/dist",
    "postversion": "git push origin master --follow-tags && npm publish"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "main": "dist/vuealert.common.js",
  "keywords": [
    "bootstrap",
    "vue",
    "vue-alert",
    "vue-alert bootstrap"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "1.0.0-beta.28",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^24.0.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.14",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-plugin-vue": "4.6.2",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "2.x",
    "webpack": "^4.29.0",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "vue": "2.x"
  }
}
